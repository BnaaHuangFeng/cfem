{
	"mesh":{
		"mode":"structured",
		"dim":2,
		"shape":"rectangular",
		"nx":50,
		"ny":50,
		"size":{
			"xmax":5.0,
			"ymax":5.0
		},
		"meshtype":"quad4",
		"savemesh":true,
		"outputfile":"step3.vtu"
	},
	"element":{
		"elmt1":{
			"type":"CPE4R",
			"set":"all"
		}
	},
	"material":{
		"mat1":{
			"type":"linearelastic",
			"parameters":{
				"E":1.0e3,
				"nu":0.3
			},
			"set":"all"
		}
	},
	"step":{
		"nlsolver":"newtonls",
		"kspsolver":"gmres",
		"preconditioner":"bjacobi",
		"dt0":0.1,
		"dtmax":1.0e-1,
		"dtmin":1.0e-12,
		"optimize-iters":3,
		"end-time":1.0e-2,
		"growth-factor":1.1,
		"cutback-factor":0.85,
		"adaptive":true,
		"maxiters":50,
		"abs-tolerance":5.0e-7,
		"rel-tolerance":5.0e-10,
		"du-tolerance":0.0
	},
	"output":{
		"field":{
			"format":"vtu",
			"interval":1,
			"variable":["vonMises-stress","stress","log-strain"]
		},
		"history":{
			"history1":{
				"format":"csv",
				"interval":1,
				"set":"load",
				"variable":["U2","RF2"]
			}
		}
	},
	"bcs":{
		"fix":{
			"type":"dirichlet",
			"dofs":[1,2],
			"bcvalue":0.0,
			"set":["bottom"]
		},
		"load":{
			"type":"dirichlet",
			"dofs":[2],
			"bcvalue":0.1,
			"set":["top"]
		}
	}
}